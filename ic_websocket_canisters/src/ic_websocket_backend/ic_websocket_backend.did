type Message = record {
  client_id: nat64;
  key: text;
  val: blob;
};

type CertMessages = record {
  messages: vec Message;
  cert: blob;
  tree: blob;
};

service : {
  "ws_register": (blob) -> ();
  "ws_open": (blob, blob) -> (bool);
  "ws_close": (nat64) -> ();
  "ws_message": (blob) -> (bool);
  "ws_get_messages": (nat64) -> (CertMessages) query;

  "ws_wipe": () -> ();
}
